   "source": [
    "import pyinputplus as pyip\n",
    "\n",
    "scor=pyip.inputInt(\"請輸入學生分數:\",min=0,max=300)\n",
    "print(scor)\n",
    "is_add = pyip.inputYesNo(\"學生是否符合加分 (y or n)\",yesVal=\"y\",noVal=\"n\",default=\"n\",blank=True)\n",
    "\n",
    "if(is_add==\"y\"):\n",
    "    scor *=1.05 \n",
    "    if scor>=300:\n",
    "        scor=300\n",
    "print(f\"學生分數是{round(scor)}\")\n",
    "\n"
   ]
  }
